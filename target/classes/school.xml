<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   <bean id="student1" class="com.raunaqpahwa.Student" init-method="initStudent" destroy-method="destroyStudent">
   		<property name="name" value="Mohit" />
   		<property name="age" value="13" />
   		<property name="address" value="Block Road" />
   </bean>
   <bean id="student2" class="com.raunaqpahwa.Student" init-method="initStudent" destroy-method="destroyStudent">
   		<property name="name" value="Harsh" />
   		<property name="age" value="12" />
   		<property name="address" value="Cross Road" />
   </bean>
   <bean id="student3" class="com.raunaqpahwa.Student" init-method="initStudent" destroy-method="destroyStudent">
   		<constructor-arg name="name" value="Abhi" />
   		<constructor-arg name="age" value="12" />
   		<constructor-arg name="address" value="Cross Road" />
   </bean>
   <bean id="school" class="com.raunaqpahwa.School">
   		<property name="name" value="Riverdale School" />
   		<property name="students">
   			<list>
   				<ref bean="student1" />
   				<ref bean="student1" />
   				<ref bean="student1" />
   			</list>
   		</property>
   </bean>
</beans>